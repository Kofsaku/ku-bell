{% render "preconnect" %}<link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
{%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
<link rel="dns-prefetch" href="https://fonts.shopifycdn.com" crossorigin>{%- endunless -%}
{%- unless settings.type_body_font.system? -%}
<link rel="preload" as="font" href="{{ settings.type_body_font | font_url }}" type="font/woff2" crossorigin>{%- endunless -%}
{%- unless settings.type_header_font.system? -%}
<link rel="preload" as="font" href="{{ settings.type_header_font | font_url }}" type="font/woff2" crossorigin>{%- endunless -%}
<link rel="preload" href="{{ 'base.css' | asset_url }}" as="style">